<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.haifeiWu.entity">

    <class name="PHCSMP_Line" table="PHCSMP_Line" lazy="false">
    
        <id name="line_Count">
            <generator class="native"/>
        </id>
        <property name="line_Used" type="java.lang.Integer">
        	<column name="line_Used" not-null="true" default="0"></column>
        </property>
        
    </class>

</hibernate-mapping>
