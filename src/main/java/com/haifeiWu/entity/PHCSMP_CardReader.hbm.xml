<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.haifeiWu.entity">

    <class name="PHCSMP_CardReader" table="PHCSMP_CardReader" lazy="false">
    
        <id name="cardReader_ID">
            <generator class="native"/>
        </id>
        
        <property name="cardReader_Name"/>
        <!--wxy增加V1.2版本的字段-->
        <property name="cardReader_Type" type="java.lang.Integer"> 
			<column name="cardReader_Type" not-null="true"/> 
		</property>
        
    </class>

</hibernate-mapping>
